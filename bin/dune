(executables
 (names main)
 (public_names watch-later)
 (libraries core async watch_later)
 (preprocess
  (pps ppx_jane)))

(rule
 (alias runtest)
 (action
  (diff main-help-for-review.org main-help-for-review.org.gen)))

(rule
 (target main-help-for-review.org.gen)
 (deps
  (:exe ./main.exe))
 (action
  (with-stdout-to
   %{target}
   (run %{exe} help -recursive -flags))))
